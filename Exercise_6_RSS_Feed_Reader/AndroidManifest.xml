<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lab.cahcet.edu.rssfeedreader">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="RSS Feed Reader"
        android:theme="@style/AppTheme">
        <activity android:name=".RSSFeedExtract">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".RSSFeedDisplay">
            <intent-filter>
                <action android:name="lab.cahcet.edu.rssfeedreader.RssFeedDisplay"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
    </application>
</manifest>